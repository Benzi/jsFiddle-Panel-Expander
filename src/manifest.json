{
  "name": "jsFiddle Panel Expander",
  "short_name": "JPE",
  "version": "1.0.2",
  "description": "Expand panels so you can code without feeling claustrophobic.",
  "author": "Benzi <hi@benzi.io>",
  "browser_action": {
    "default_icon": "icons/48.png"
  },
  "icons": {
    "16": "icons/16.png",
    "24": "icons/24.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "96": "icons/96.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
  },
  "content_scripts": [{
    "js": ["loader.js"],
    "matches": ["*://jsfiddle.net/*"],
    "run_at": "document_end"
  }],
  "web_accessible_resources": ["main.js"],
  "permissions": ["*://jsfiddle.net/*"],
  "manifest_version": 2
}
